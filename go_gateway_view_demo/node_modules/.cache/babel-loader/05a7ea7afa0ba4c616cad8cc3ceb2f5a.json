    if (!value) {\n        this.deleteTodo({\n          todo\n        })\n      } else if (this.editing) {\n        this.editTodo({\n          todo,\n          value\n        })\n        this.editing = false\n      }\n    },\n    cancelEdit(e) {\n      e.target.value = this.todo.text\n      this.editing = false\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/dashboard/admin/components/TodoList"}]}